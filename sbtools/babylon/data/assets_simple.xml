<?xml version="1.0" encoding="utf-8"?>

<!-- ASSETS CAN BE AN EMPTY OR HAVE GEOMETRY-->
<!-- ASSETS CAN BE NESTED, THIS IS NORMALLY AN EMPTY CONTAINER AND CHILDREN WITH GEOMETRY-->
<!-- KEY is the string used to stor and reference the asset-->
<!-- TRANSFORM can be affected by filters-->
<!-- TRANSFORM can be omitted and the object will get a zerod out position : 0,0,0 , zerod rotation 0,0,0 and oned scaled , 1,1,1-->
<!-- L,R,S of children can also be omitted individually and the same logic as above applies , they will just get a zero or 1 vector default-->
<!-- DISPLAY can be affected by filters-->
<!-- DISPLAY can be omitted if you know it is not affect by filters-->
<!-- URL can be left out and it wll be an empty or point to a child of a objct already loaded, URL will point to a .glb to load for geometry-->
<!-- PARENT tag is opional to say if the asset needs to be paented to another object , if omitted it is added to the scene root-->




<ROOT>

  <BUILD_TARGET><![CDATA[processData]]></BUILD_TARGET>



  <!-- room-->
  <ASSET>
    <LOAD_ASSET_ON_START><![CDATA[true]]></LOAD_ASSET_ON_START>
    <KEY><![CDATA[room]]></KEY>
    <URL><![CDATA[assets/glb/room.glb]]></URL>

  </ASSET>
  <!-- basket-->
  <ASSET>
    <KEY><![CDATA[basket]]></KEY>
    <TRANSFORM>
      <DEFAULT>
        <L><![CDATA[2.22967,-2.08902,0]]></L>
      </DEFAULT>
    </TRANSFORM>
    <URL><![CDATA[assets/glb/basket.glb]]></URL>
  </ASSET>
